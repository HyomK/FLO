<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

    </style>
    <style name="roundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">30dp</item>
        <item name="android:background">@color/cardview_dark_background</item>
    </style>

    <style name="ButtonStyle" parent="Widget.AppCompat.Button">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:text">"전체듣기"</item>

    </style>
    <style name="tab_text" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="locker_tab_text" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">15sp</item>
        <item name="android:tabStripEnabled">false</item>

    </style>

    <style name="FilledBoxEndIconTint" parent="Widget.MaterialComponents.TextInputLayout.FilledBox">
        <item name="endIconTint">@color/black</item>
        <item name="background">@color/white</item>
        <item name="colorPrimary">@color/black</item>
        <item name="colorOnSurface">@color/black</item>
    </style>

</resources>